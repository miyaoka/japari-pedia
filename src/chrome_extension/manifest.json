{
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "manifest_version": 2,
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/19.png",
      "38": "icon/38.png"
    }
  },
  "background": {
    "scripts": [ "event.js" ],
    "persistent": false
  },
  "permissions": [
    "https://www.wikidata.org/",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://*.wikipedia.org/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
